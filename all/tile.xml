<project>
    <build>
        <plugins>
            <plugin>
                <groupId>io.repaint.maven</groupId>
                <artifactId>tiles-maven-plugin</artifactId>
                <version>${tiles-maven-plugin.version}</version>
                <extensions>true</extensions>
                <configuration>
                    <tiles>
                        <tile>net.kemitix.tiles:maven-plugins:1.0.0</tile>
                        <tile>net.kemitix.tiles:enforcer:1.0.0</tile>
                        <tile>net.kemitix.tiles:compiler:1.0.0</tile>
                        <tile>net.kemitix.tiles:huntbugs:1.0.0</tile>
                        <tile>net.kemitix.tiles:pmd:1.0.0</tile>
                        <tile>net.kemitix.tiles:digraph:1.0.0</tile>
                        <tile>net.kemitix.tiles:testing:1.0.0</tile>
                        <tile>net.kemitix.tiles:coverage:1.0.0</tile>
                        <tile>net.kemitix.tiles:pitest:1.0.0</tile>
                    </tiles>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>
